{{ form_start(form, {'attr' : {'enctype': 'multipart/form-data','class': 'w-full'} }) }}
<div class="grid gap-y-6">
    <div class="grid gap-y-4">
        <div class="flex flex-col">
            {{ form_label(form.value) }}
            {{ form_widget(form.value, {'attr': {'class': 'bg-white rounded'}}) }}
        </div>
        <div class="flex flex-col">
            {{ form_label(form.content) }}
            {{ form_widget(form.content, {'attr': {'class': 'bg-white rounded'}}) }}
        </div>
    </div>
</div>
<div class="flex justify-end mt-4">
    {{ component('Global:Button',{
        tag: 'button',
        text: 'Ajouter',
        class: 'primary'
    }) }}
</div>
{{ form_end(form) }}