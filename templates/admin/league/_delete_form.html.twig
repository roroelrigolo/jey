<form method="post" action="{{ path('app_admin_league_delete', {'id': league.id}) }}"
      onsubmit="return confirm('Voulez vous vraiment supprimer cette ligue ?');">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ league.id) }}">
    {{ component('Global:Button',{
        tag: 'button',
        text: 'Supprimer',
        class: 'delete'
    }) }}
</form>
