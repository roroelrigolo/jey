<form method="post" action="{{ path('app_admin_contact_delete', {'id': contact.id}) }}"
      onsubmit="return confirm('Voulez vous vraiment supprimer cette demande de contact ?');">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ contact.id) }}">
    {{ component('Global:Button',{
        tag: 'button',
        text: 'Supprimer',
        class: 'delete'
    }) }}
</form>
